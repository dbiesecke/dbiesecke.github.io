<settings>
	<setting id="appcode" type="text" label="30001" default="None"/>
	<setting id="lineid" type="text" label="30002" default="None"/>
	<setting id="qrcode" type="text" label="qrcode" visible="false" default="None"/>
	<setting id="m3uurl" type="text" label="m3u" visible="false" default="None"/>
	<setting id="epgurl" type="text" label="epg" visible="false" default="None"/>
    
<category label="General">
<setting id="busybox" label="busybox exe" type="file" default="/usr/bin/busybox"/>
</category>
<category label="Service">
<setting id="notification" label="Update Notifications" type="bool" default="true" />
<setting id="service" label="Service" type="bool" default="true" />
<setting id="service.startup" label="Update on Login" type="bool" default="true" visible="eq(-1,true)"/>
<setting id="service.type" label="Service Schedule Type" type="enum" default="Time" visible="eq(-2,true)" values="None|Period|Time" />
<setting id="service.interval" label="Interval" type="number" default="24"  visible="eq(-3,true)+eq(-1,1)" />
<setting id="service.time" type="time" label="Service Time" default="03:33" visible="eq(-4,true)+eq(-2,2)"/>
</category>

</settings>
